<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="datos.Organizacion" table="organizacion">

<id column="idOrganizacion" name="idOrganizacion">
<generator class="identity"/>
</id>
<property column="nombre" name="nombre" type="string" />
<property column="razonSocial" name="razonSocial" type="string" />
<property column="cuit" name="cuit" type="string" />
<property column="direccion" name="direccion" type="string" />
<property column="ubicacion" name="ubicacion" type="string" />
</class>
</hibernate-mapping>